#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "entidades.h"

int main(void) {

	int opcion;
	est_fecha date;
	est_electro aparatito;
	printf("Ingrese una opcion: ");
	scanf("%d", &opcion);
	switch(opcion)
	{
		case 1:
			date = cargarFecha();
			mostrarFecha(date);
			system ("pause");
			break;
		case 2:
			aparatito = cargarElectrodomestico();
			mostrarElectrodomestico(aparatito);
			system ("pause");
			break;
	}
	return EXIT_SUCCESS;
}
