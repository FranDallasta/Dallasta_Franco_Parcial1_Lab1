#ifndef ENTIDADES_H_
#define ENTIDADES_H_

typedef struct{
		int dia;
		int mes;
		int anio;
	}est_fecha;

typedef struct{
		int id_electro;
		int serie_electro;
		char marca_electro;
		est_fecha anio_electro;
	} est_electro;

typedef struct{
		int id_servicio;
		char desc_servicio [25];
		float precio_servicio;
	}est_servicio;

typedef struct{
		int id_reparacion;
		est_electro serie_reparacion;
		est_servicio servicio_reparacion;
		est_fecha fecha_reparacion;
	}est_reparacion;

#endif /* ENTIDADES_H_ */

est_fecha cargarFecha();
void mostrarFecha(est_fecha fecha);
est_electro cargarElectrodomestico();
void mostrarElectrodomestico(est_electro aparato);
